<Window x:Class="LARC.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dvsku="http://dvsku.com/winfx/xaml/controls"
        xmlns:views="clr-namespace:LARC.GUI.Views"
        mc:Ignorable="d"
        Title="LARC" Background="#1E1E1E" Height="200" Width="650" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Background="#1E1E1E">
        <TabControl Background="#1E1E1E" Margin="0, 5, 0, 0" BorderBrush="Transparent">
            <TabItem Header="Region" >
                <views:MainView  />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
